{
  "name": "slot-engine-x",
  "version": "1.0.0",
  "description": "A fully functional slot machine engine for web environments",
  "type": "module",
  "main": "./dist/SlotEngine.js",
  "types": "./dist/SlotEngine.d.ts",
  "exports": {
    ".": {
      "import": "./dist/SlotEngine.js",
      "types": "./dist/SlotEngine.d.ts"
    }
  },
  "scripts": {
