  "keywords": [
    "slot-machine",
    "game-engine",
    "casino",
    "browser",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "canvas": "^3.2.0",
    "http-server": "^14.1.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
